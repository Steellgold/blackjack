{
  "scripts": {
      "dev": "open-cli http://localhost:3000 && dotenv -- pnpm --filter=@core-app/* --recursive run dev",
      "build": "dotenv -- pnpm --filter=@core* --recursive run build",
      "start:website": "pnpm --filter=@core-app/website run start",
      "start:socket": "pnpm --filter=@core-app/socket run start"
  },
  "dependencies": {
      "dotenv": "^16.4.5",
      "dotenv-cli": "^7.4.1",
      "open-cli": "^8.0.0"
  },
  "engines": {
      "node": ">=20.0.0",
      "pnpm": ">=9.0.0"
  }
}